{
  "name": "@pactflow/openapi-pact-comparator",
  "version": "0.0.1",
  "description": "An OpenAPI Specification and Pact Comparison library",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/src/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup --config",
    "changeset:add": "changeset add",
    "changeset:publish": "changeset publish",
    "changeset:version": "changeset version",
    "lint": "eslint --max-warnings=0",
    "postinstall": "patch-package",
    "prepare": "npm run build",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --cache --list-different --write .",
    "start": "node dist/cli.mjs",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --project tsconfig.json"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pactflow/openapi-pact-comparator.git"
  },
  "author": {
    "name": "Voon Siong Wong",
    "email": "voon.wong@smartbear.com"
  },
  "license": "Apache-2.0",
  "type": "module",
  "devDependencies": {
    "@apidevtools/swagger-parser": "10.1.1",
    "@changesets/cli": "^2.28.1",
    "@eslint/js": "9.22.0",
    "@pactflow/swagger-mock-validator": "14.5.0",
    "@rollup/plugin-commonjs": "28.0.3",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-typescript": "12.1.2",
    "@sinclair/typebox": "0.34.30",
    "@types/js-yaml": "4.0.9",
    "@types/lodash-es": "4.17.12",
    "@types/node": "22.13.10",
    "@types/qs": "6.9.18",
    "@vitest/coverage-v8": "3.0.9",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "find-my-way": "9.2.0",
    "js-yaml": "4.1.0",
    "lodash-es": "4.17.21",
    "parse-multipart-data": "1.5.0",
    "patch-package": "8.0.0",
    "prettier": "3.5.3",
    "qs": "6.14.0",
    "rollup": "4.36.0",
    "tslib": "2.8.1",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.1",
    "vitest": "3.0.9"
  },
  "imports": {
    "#compare/*": "./src/compare/*.ts",
    "#documents/*": "./src/documents/*.ts",
    "#results/*": "./src/results/*.ts",
    "#transform/*": "./src/transform/*.ts",
    "#utils/*": "./src/utils/*.ts"
  }
}
