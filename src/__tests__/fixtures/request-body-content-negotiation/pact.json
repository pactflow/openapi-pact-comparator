{
  "consumer": { "name": "consumer" },
  "provider": { "name": "provider" },
  "interactions": [
    {
      "description": "defaults to first matching content-type",
      "request": {
        "method": "POST",
        "path": "/animals",
        "headers": {
          "content-type": "application/json"
        },
        "body": {
          "name": "Rover",
          "genus": "Canis"
        }
      },
      "response": {
        "status": 201
      }
    },
    {
      "description": "success: POST v1",
      "request": {
        "method": "POST",
        "path": "/animals",
        "headers": {
          "content-type": "application/json;version=1.0.0"
        },
        "body": {
          "name": "Rover",
          "genus": "Canis"
        }
      },
      "response": {
        "status": 201
      }
    },
    {
      "description": "success: POST v2",
      "request": {
        "method": "POST",
        "path": "/animals",
        "headers": {
          "content-type": "application/json;version=2.0.0"
        },
        "body": {
          "name": "Rover",
          "species": "C. familiaris"
        }
      },
      "response": {
        "status": 201
      }
    }
  ]
}
