{
  "consumer": { "name": "consumer" },
  "provider": { "name": "provider" },
  "interactions": [
    {
      "description": "invalid path, should be integer",
      "request": {
        "method": "GET",
        "path": "/integer/abc"
      },
      "response": {
        "status": 200
      }
    },
    {
      "description": "success: integer path parameter",
      "request": {
        "method": "GET",
        "path": "/integer/1"
      },
      "response": {
        "status": 200
      }
    },
    {
      "description": "invalid path, should be boolean",
      "request": {
        "method": "GET",
        "path": "/boolean/abc"
      },
      "response": {
        "status": 200
      }
    },
    {
      "description": "success: boolean path parameter",
      "request": {
        "method": "GET",
        "path": "/boolean/true"
      },
      "response": {
        "status": 200
      }
    },
    {
      "description": "invalid request using simple style with exploded path parameters",
      "request": {
        "method": "GET",
        "path": "/simple/explode-true/1,2,abc"
      },
      "response": {
        "status": 200
      }
    },
    {
      "description": "valid request using simple style with exploded path parameters",
      "request": {
        "method": "GET",
        "path": "/simple/explode-true/1,2,3"
      },
      "response": {
        "status": 200
      }
    },
    {
      "description": "invalid request using simple style with non-exploded path parameters",
      "request": {
        "method": "GET",
        "path": "/simple/explode-false/1,2,abc"
      },
      "response": {
        "status": 200
      }
    },
    {
      "description": "valid request using simple style with non-exploded path parameters",
      "request": {
        "method": "GET",
        "path": "/simple/explode-false/1,2,3"
      },
      "response": {
        "status": 200
      }
    },
    {
      "description": "invalid request using exploded path parameters",
      "request": {
        "method": "GET",
        "path": "/exploded-object/string=abc,number=abc"
      },
      "response": {
        "status": 200
      }
    },
    {
      "description": "valid request using exploded path parameters",
      "request": {
        "method": "GET",
        "path": "/exploded-object/string=abc,number=123"
      },
      "response": {
        "status": 200
      }
    }
